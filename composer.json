{
  "name": "vatsake/php-asic-e",
  "description": "Lightweight PHP library for creating and validating XAdES-T and ASiC-E digital signatures (compatible with Estonian DigiDoc).",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Vatsake\\AsicE\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vatsake\\AsicE\\tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Erko Sten HÃ¼rges",
      "email": "erko124@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "phpseclib/phpseclib": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12",
    "squizlabs/php_codesniffer": "^4.0"
  },
  "scripts": {
    "test": "phpunit --coverage-html coverage",
    "lint": "phpcs src"
  },
  "archive": {
    "exclude": [
      "/tests",
      "/phpcs.xml",
      "phpunit.xml"
    ]
  },
  "support": {
    "issues": "https://github.com/vatsake/php-asic-e/issues",
    "source": "https://github.com/vatsake/php-asic-e",
    "docs": "https://github.com/vatsake/php-asic-e#readme"
  },
  "homepage": "https://github.com/vatsake/php-asic-e",
  "keywords": [
    "xades",
    "digital signature",
    "asic-e",
    "php",
    "security",
    "timestamp",
    "ocsp",
    "xml",
    "signature"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true
}
