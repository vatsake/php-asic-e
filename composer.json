{
  "name": "vatsake/php-asice",
  "description": "XAdES/ASiC-E digital signature library for PHP",
  "type": "library",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "Vatsake\\AsicE\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vatsake\\AsicE\\tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Erko Sten HÃ¼rges",
      "email": "erko124@gmail.com"
    }
  ],
  "require": {
